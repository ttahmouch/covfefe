{
    "$type": "div",
    "$props": {"id": "root", "data-style": "root"},
    "$children": [
        {
            "$type": "nav",
            "$props": {"data-style": "navigation_bar"},
            "$children": [
                {
                    "$type": "div",
                    "$props": {"data-style": "navigation_bar_back_button", "data-state": "back"}
                },
                {
                    "$type": "div",
                    "$props": {"data-style": "navigation_bar_back_button", "data-state": "forward"}
                },
                {
                    "$type": "div",
                    "$props": {"data-style": "navigation_bar_title", "data-state": "page_title"}
                }
            ]
        },
        {
            "$type": "div",
            "$props": {"id": "header", "data-style": "header"},
            "$children": [
                {
                    "$type": "img",
                    "$props": {
                        "id": "hero-image",
                        "src": "./dashboard_hero_image_gmc.png",
                        "data-style": "hero-image"
                    }
                }
            ]
        },
        {
            "$type": "div",
            "$props": {"id": "garage-door", "data-style": "garage-door"},
            "$children": [
                {
                    "$type": "p",
                    "$props": {"data-style": "garage-door-text", "data-state": "selected_vehicle"}
                }
            ]
        },
        {
            "$type": "div",
            "$props": {"id": "footer", "data-style": "footer"},
            "$children": [
                {
                    "$type": "div",
                    "$props": {"data-style": "card"},
                    "$children": [
                        {
                            "$type": "header",
                            "$props": {"data-style": "card_header", "data-state": "card_title"}
                        },
                        {
                            "$type": "div",
                            "$props": {"data-style": "card_main_content"},
                            "$children": [
                                {
                                    "$type": "Json",
                                    "$props": {"data-state": "titles_dictionary", "data-bind": "json"}
                                },
                                {
                                    "$type": "form",
                                    "$props": {
                                        "data-event": "onSubmit",
                                        "data-action": "async_read",
                                        "data-state": "titles_dictionary",
                                        "data-state-type": "dictionary"
                                    },
                                    "$children": [
                                        {
                                            "$type": "div",
                                            "$children": [
                                                {
                                                    "$type": "label",
                                                    "$props": {"data-state": "title_form_label"}
                                                },
                                                {
                                                    "$type": "input",
                                                    "$props": {"name": "title"}
                                                }
                                            ]
                                        },
                                        {
                                            "$type": "div",
                                            "$children": [
                                                {
                                                    "$type": "button",
                                                    "$props": {"type": "submit", "data-state": "title_form_submit_button_label"}
                                                }
                                            ]
                                        }
                                    ]
                                }
                            ]
                        }
                    ]
                }
            ]
        }
    ]
}