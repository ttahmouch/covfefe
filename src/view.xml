<div id="root" data-style="root">
    <nav data-style="navigation_bar">
        <div data-style="navigation_bar_back_button" data-state="back"/>
        <div data-style="navigation_bar_back_button" data-state="forward"/>
        <div data-style="navigation_bar_title" data-state="page_title"/>
    </nav>
    <div id="header" data-style="header">
        <img id="hero-image"
             src="./dashboard_hero_image_gmc.png"
             data-style="hero-image"/>
    </div>
    <div id="garage-door" data-style="garage-door">
        <p data-style="garage-door-text" data-state="selected_vehicle"/>
    </div>
    <div id="footer" data-style="footer">
        <main data-style="main_content">
            <div data-style="card">
                <header data-style="card_header" data-state="card_title"/>
                <div data-style="card_main_content">
                    <Json data-state="titles_dictionary" data-bind="json"/>
                    <form data-event="onSubmit"
                          data-action="async_read"
                          data-state="titles_dictionary"
                          data-state-type="dictionary">
                        <div>
                            <label data-state="title_form_label"/>
                            <input name="title"/>
                        </div>
                        <div>
                            <button type="submit" data-state="title_form_submit_button_label"/>
                        </div>
                    </form>
                </div>
            </div>
        </main>
    </div>
</div>