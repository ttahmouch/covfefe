{
  "navigation_bar": {
    "height": "3em",
    "width": "100%",
    "backgroundColor": "#BD261A",
    "display": "flex",
    "alignItems": "center",
    "color": "white",
    "position": "fixed",
    "top": 0
  },
  "navigation_bar_back_button": {
    "height": "100%",
    "width": "3em",
    "display": "flex",
    "alignItems": "center",
    "justifyContent": "center",
    "color": "white"
  },
  "navigation_bar_title": {
    "height": "100%",
    "width": "100%",
    "display": "flex",
    "alignItems": "center",
    "color": "white"
  },
  "main_content": {
    "display": "flex",
    "flexDirection": "column",
    "overflowY": "scroll",
    "overflowX": "hidden",
    "marginTop": "3em"
  },
  "card": {
    "display": "flex",
    "flexDirection": "column",
    "height": "9em",
    "borderStyle": "solid",
    "borderColor": "#c3c3c3",
    "borderRadius": "5px",
    "margin": "0.5em",
    "backgroundColor": "#FFF"
  },
  "card_header": {
    "margin": "1em",
    "fontSize": ".75em"
  },
  "card_main_content": {
    "display": "flex",
    "margin": ".5em",
    "backgroundColor": "#F00",
    "height": "100%"
  },
  "root": {
    "display": "flex",
    "flexDirection": "column",
    "width": "100%",
    "height": "100%"
  },
  "header": {
    "width": "100%",
    "height": "300px",
    "background": "white"
  },
  "hero-image": {
    "width": "100%",
    "height": "100%",
    "objectFit": "cover"
  },
  "garage-door": {
    "display": "flex",
    "flexDirection": "row",
    "width": "100%",
    "height": "75px",
    "background": "black",
    "alignItems": "center",
    "justifyContent": "center"
  },
  "garage-door-text": {
    "color": "white",
    "fontFamily": "sans-serif",
    "fontSize": "2em"
  },
  "footer": {
    "width": "100%",
    "height": "500px",
    "background": "gray"
  }
}