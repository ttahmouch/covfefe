{
    "$schema": "http://json-schema.org/draft-07/schema#",
    "type": "object",
    "required": ["status", "headers", "body"],
    "properties": {
        "status": {
            "type": "integer",
            "const": 200
        },
        "headers": {
            "type": "object",
            "properties": {
                "content-type": {
                    "type": "string",
                    "pattern": "application[/]json"
                },
                "content-length": {
                    "type": "string",
                    "pattern": "^([0-9]+)$"
                }
            }
        },
        "body": {
            "$schema": "http://json-schema.org/schema#",
            "type": "object",
            "properties": {"titles": {"type": "array"}}
        }
    }
}
