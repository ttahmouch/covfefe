{
    "$compose": [
        {
            "$compose": "read",
            "$type": "json_path",
            "$value": "$['response']['body']",
            "$default": {}
        },
        {
            "$compose": "update",
            "$type": "spread",
            "$value": {
                "$compose": "create",
                "$value": {
                    "titles": [
                        {
                            "$compose": "read",
                            "$type": "json_path",
                            "$value": "$['response']['body']['titles']",
                            "$default": []
                        },
                        {
                            "$compose": "update",
                            "$type": "spread",
                            "$value": {
                                "$compose": "create",
                                "$value": [
                                    {
                                        "title": {
                                            "$compose": "read",
                                            "$type": "json_path",
                                            "$value": "$['app']['$states']['title']",
                                            "$default": ""
                                        }
                                    },
                                    {
                                        "title": {
                                            "$compose": "read",
                                            "$type": "json_path",
                                            "$value": "$['view']['title']['0']",
                                            "$default": ""
                                        }
                                    }
                                ],
                                "$default": [
                                    {"title": ""},
                                    {"title": ""}
                                ]
                            },
                            "$default": [
                                {"title": ""},
                                {"title": ""}
                            ]
                        }
                    ]
                },
                "$default": {
                    "titles": [
                        {"title": ""},
                        {"title": ""}
                    ]
                }
            },
            "$default": {
                "titles": [
                    {"title": ""},
                    {"title": ""}
                ]
            }
        }
    ]
}