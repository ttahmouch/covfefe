{
    "$request": {
        "$method": "GET",
        "$uri": {
            "$default": "/",
            "$compose": "request_titles_dictionary_uri"
        },
        "$headers": {
            "header": {
                "$default": "",
                "$compose": "request_titles_dictionary_header"
            }
        },
        "$events": {
            "load": {"$event": "load"},
            "loadstart": {"$event": "request_titles_dictionary_loadstart"},
            "progress": {"$event": "progress"},
            "error": {"$event": "error"},
            "abort": {"$event": "abort"},
            "timeout": {"$event": "timeout"},
            "loadend": {"$event": "request_titles_dictionary_loadend"},
            "200": {"$event": "request_titles_dictionary_200"}
        }
    }
}