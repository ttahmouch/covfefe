{
  "name": "expo-disneyplus",
  "version": "0.0.1",
  "private": true,
  "main": "src/index.js",
  "scripts": {
    "copy_new_covfefe": "cp ../index.js ./node_modules/covfefe",
    "move_old_covfefe": "mv ./node_modules/covfefe/index.js ./node_modules/covfefe/old.index.js",
    "take_old_covfefe": "mv ./node_modules/covfefe/old.index.js ./node_modules/covfefe/index.js",
    "reset": "npm run take_old_covfefe",
    "prestart": "npm run move_old_covfefe && npm run copy_new_covfefe",
    "dev": "expo start",
    "start": "expo start",
    "lint": "eslint ./src",
    "android": "expo start --android",
    "ios": "expo start --ios",
    "web": "expo start --web",
    "web-build": "expo build:web",
    "eject": "expo eject"
  },
  "dependencies": {
    "@react-native-community/masked-view": "0.1.10",
    "covfefe": "git+https://github.com/ttahmouch/covfefe.git#feature/jsonml+disneyplus+events+react17",
    "expo": "^42.0.0",
    "expo-constants": "~11.0.1",
    "history": "^5.0.0",
    "prop-types": "^15.7.2",
    "react": "16.13.1",
    "react-dom": "^16.13.1",
    "react-native": "https://github.com/expo/react-native/archive/sdk-42.0.0.tar.gz",
    "react-native-gesture-handler": "~1.10.2",
    "react-native-reanimated": "~2.2.0",
    "react-native-safe-area-context": "3.2.0",
    "react-native-screens": "~3.4.0",
    "react-native-snap-carousel": "^4.0.0-beta.6",
    "react-native-svg": "12.1.1",
    "react-native-web": "~0.13.12",
    "react-navigation": "^4.4.4",
    "react-navigation-stack": "^2.10.4",
    "react-navigation-tabs": "^2.11.0",
    "redux-devtools-extension": "^2.13.9"
  },
  "devDependencies": {
    "@babel/core": "~7.9.0",
    "babel-eslint": "^10.1.0",
    "babel-preset-expo": "8.3.0",
    "eslint": "^7.8.1",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-config-prettier": "^6.11.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-import-helpers": "^1.1.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-prettier": "^3.1.4",
    "eslint-plugin-react": "^7.20.6",
    "eslint-plugin-react-hooks": "^4.2.0",
    "prettier": "^2.1.1"
  },
  "eslintIgnore": [
    "babel.config.js"
  ]
}
