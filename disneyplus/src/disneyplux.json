  "$styles": {
    "header": {
      "alignItems": "flex-start",
      "flexDirection": "row",
      "justifyContent": "space-between",
      "paddingBottom": 4,
      "paddingHorizontal": 16,
      "paddingTop": 30
    },
    "profile_row": {
      "borderBottomColor": "#A4A3A2",
      "borderBottomWidth": 1,
      "flexDirection": "row",
      "justifyContent": "space-between",
      "marginLeft": 16,
      "paddingRight": 16,
      "paddingVertical": 20
    },
    "profile_row_text": {
      "color": "#FFFFFF",
      "flex": 2,
      "fontWeight": "normal",
      "fontSize": 16
    },
    "header_back_button_with_text": {
      "color": "#FFFFFF",
      "fontWeight": "bold"
    },
    "header_title": {
      "flex": 4,
      "height": 35,
      "justifyContent": "flex-end"
    },
    "home_row_item": {
      "height": 130,
      "width": 93,
      "borderRadius": 4,
      "marginRight": 8,
      "overflow": "hidden"
    }
  },
  "$events": {
    "on_press_logo": [
      {
        "$action": "create_$states_items",
        "$value": {
          "items": {
            "should_show_modal": false
          }
        }
      }
    ]
  },
